{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "targets": {
          "chrome": "64",
          "edge": "79",
          "firefox": "78",
          "opera": "51"
        }
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-macros"
    ],
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "h",
        "pragmaFrag": "Fragment"
      }
    ]
  ]
}
